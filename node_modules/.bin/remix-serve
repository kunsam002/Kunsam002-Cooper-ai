#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kunsam002/Projects/cooperai-tech-interview-challenge/node_modules/.pnpm/@remix-run+serve@2.12.0_typescript@5.6.2/node_modules/@remix-run/serve/dist/node_modules:/Users/kunsam002/Projects/cooperai-tech-interview-challenge/node_modules/.pnpm/@remix-run+serve@2.12.0_typescript@5.6.2/node_modules/@remix-run/serve/node_modules:/Users/kunsam002/Projects/cooperai-tech-interview-challenge/node_modules/.pnpm/@remix-run+serve@2.12.0_typescript@5.6.2/node_modules/@remix-run/node_modules:/Users/kunsam002/Projects/cooperai-tech-interview-challenge/node_modules/.pnpm/@remix-run+serve@2.12.0_typescript@5.6.2/node_modules:/Users/kunsam002/Projects/cooperai-tech-interview-challenge/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kunsam002/Projects/cooperai-tech-interview-challenge/node_modules/.pnpm/@remix-run+serve@2.12.0_typescript@5.6.2/node_modules/@remix-run/serve/dist/node_modules:/Users/kunsam002/Projects/cooperai-tech-interview-challenge/node_modules/.pnpm/@remix-run+serve@2.12.0_typescript@5.6.2/node_modules/@remix-run/serve/node_modules:/Users/kunsam002/Projects/cooperai-tech-interview-challenge/node_modules/.pnpm/@remix-run+serve@2.12.0_typescript@5.6.2/node_modules/@remix-run/node_modules:/Users/kunsam002/Projects/cooperai-tech-interview-challenge/node_modules/.pnpm/@remix-run+serve@2.12.0_typescript@5.6.2/node_modules:/Users/kunsam002/Projects/cooperai-tech-interview-challenge/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@remix-run/serve/dist/cli.js" "$@"
else
  exec node  "$basedir/../@remix-run/serve/dist/cli.js" "$@"
fi
